{
    "name": "charescape/php-functions",
    "type": "library",
    "description": "Convenient and commonly used functions for PHP",
    "keywords": [],
    "homepage": "https://github.com/charescape/php-functions",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "charescape",
            "email": "charescape@outlook.com",
            "homepage": "https://github.com/charescape",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/string": "^5.4||^6.0||^7.0"
    },
    "require-dev": {
        "league/commonmark": "^2.5",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "files": [
            "src/functions.php",
            "src/is.php",
            "src/typecast.php",
            "src/internal.php",
            "src/integrations.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Charescape\\PhpFunctions\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "redo-install": [
            "rm -f composer.lock",
            "@composer self-update -vvv",
            "@composer install -vvv"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
